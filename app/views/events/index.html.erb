<div class="list-group">
  <h4 class="list-group-item active">
    <%= t('desided events') %>
  </h4>
  <% if @events.any? %>
    <% @events.each do |ev| %>
      <%= link_to ev.title, event_path(ev), class: "list-group-item" %>
    <% end %>
    <div class="text-center">
      <%= paginate @events, param_name: :events_page %>
    </div>
  <% else %>
    <p class="list-group-item"><%= t('there is no desided event') %></p>
  <% end %>
</div>